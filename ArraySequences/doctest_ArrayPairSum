>>> from ArraySequences import ArrayPairSum as aps

>>> aps.pair_sumV1([1,9,2,8,3,7,4,6,5,5,13,14,11,13,-1], 10)
6
>>> aps.pair_sumV1([1,2,3,1], 3)
1
>>> aps.pair_sumV1([1,3,2,2], 4)
2

>>> aps.pair_sumV2([1,9,2,8,3,7,4,6,5,5,13,14,11,13,-1], 10)
6
>>> aps.pair_sumV2([1,2,3,1], 3)
1
>>> aps.pair_sumV2([1,3,2,2], 4)
2


